# Git shortcuts
alias gs="git status"
alias gc="git commit"
alias gg="git log"
alias gsu="git stash -u"
alias gsp="git stash pop"
alias grc="git rebase --continue"
alias macup="sh ~/code/thoughtbot/laptop/mac 2>&1 | tee ~/laptop.log"

# GPG
alias gpg="gpg2"

# Sublime
alias s="subl"

alias trust="mkdir -p .git/safe"
